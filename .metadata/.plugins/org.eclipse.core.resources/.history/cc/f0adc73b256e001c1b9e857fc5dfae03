
public class TesteSistema {
	
	public static void main(String[] args) {
		Gerente g = new Gerente ();
		g.setSenha(2222);
		
		Administrador adm = new Administrador ();
		adm.setSenha(333);
		
		Designer d = new Designer ();
		d.setSenha(222);
		
		SistemaInterno si = new SistemaInterno();
		si.autentica(g);
		si.autentica(adm);
		si.autentica(d);
	}

}
