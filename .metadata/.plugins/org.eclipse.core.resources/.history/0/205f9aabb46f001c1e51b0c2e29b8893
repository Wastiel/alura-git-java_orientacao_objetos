
public class Cliente implements Autenticavel {

	private AutenticacaoUtil autenticador;
	
	
	public void setSenha(int senha) {
		this.autenticador = new AutenticacaoUtil();		
	}

	@Override
	public boolean autentica(int senha) {
		if(this.senha == senha) {
			return true;
		} else {
			return false;
		} 
		
	}

}
